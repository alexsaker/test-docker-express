{
  "name": "test-docker-express",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/test-docker-express.min.js",
  "scripts": {
    "start": "node dist/test-docker-express.min.js",
    "clean": "rimraf dist build *tgz",
    "lint": "tslint src/**/*.ts",
    "build:dev": "npm run clean && cross-env NODE_ENV=dev webpack",
    "serve:dev": "nodemon  ./build/test-docker-express.js",
    "watch:dev":
      "npm-run-all clean lint --parallel  \"build:dev -- --watch\" serve:dev",
    "build:prod": "npm run clean && cross-env NODE_ENV=prod webpack",
    "serve:prod": "nodemon --watch  dist/test-docker-express.min.js",
    "watch:prod":
      "npm-run-all clean lint --parallel \"build:prod -- --watch\" serve:prod"
  },
  "keywords": ["docker", "nodejs", "express"],
  "author": "Alexandre Saker",
  "email": "saker.alexandre@gmail.com",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/node": "^9.4.4",
    "babel-cli": "^6.22.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "nodemon": "1.14.12",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
